&timers8 {
	status = "okay";
	st,prescaler = <10000>;

	pwm_V: pwm {
		status = "okay";
		pinctrl-0 = <&tim8_ch1_pi5>;
		pinctrl-names = "default";
	};
	pwm_U: pwm {
		status = "okay";
		pinctrl-0 = <&tim8_ch2_pi6>;
		pinctrl-names = "default";
	};
	pwm_T: pwm {
		status = "okay";
		pinctrl-0 = <&tim8_ch3_pi7>;
		pinctrl-names = "default";
	};
	pwm_S: pwm {
		status = "okay";
		pinctrl-0 = <&tim8_ch4_pi2>;
		pinctrl-names = "default";
	};
};
